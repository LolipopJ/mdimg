{
  "name": "mdimg",
  "version": "1.1.1",
  "description": "Covert Markdown or HTML to image",
  "keywords": [
    "markdown",
    "html",
    "image",
    "md2img",
    "html2img"
  ],
  "engines": {
    "node": ">= 18.0.0"
  },
  "main": "lib/mdimg.js",
  "module": "lib/mdimg.mjs",
  "bin": {
    "mdimg": "bin/mdimg.js"
  },
  "files": [
    "README.md",
    "bin/",
    "lib/",
    "src/",
    "template/css/",
    "template/html/"
  ],
  "scripts": {
    "mdimg": "node ./bin/mdimg.js",
    "test": "jest --roots test --testTimeout 30000",
    "build": "npm run rollup:ts && npm run rollup:sass",
    "rollup:ts": "rollup -c rollup.config.js --bundleConfigAsCjs",
    "rollup:sass": "rollup -c rollup.sass.config.js --bundleConfigAsCjs",
    "lint": "eslint .",
    "prettier": "prettier --write **/* --ignore-unknown"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LolipopJ/mdimg.git"
  },
  "author": "LolipopJ",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/LolipopJ/mdimg/issues"
  },
  "homepage": "https://github.com/LolipopJ/mdimg#readme",
  "dependencies": {
    "cheerio": "^1.0.0-rc.12",
    "commander": "^12.1.0",
    "marked": "^13.0.2",
    "puppeteer": "^22.13.1"
  },
  "devDependencies": {
    "@babel/core": "^7.24.9",
    "@babel/eslint-parser": "^7.24.8",
    "@babel/node": "^7.24.8",
    "@babel/preset-env": "^7.24.8",
    "@eslint/js": "^9.7.0",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-commonjs": "^26.0.1",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-terser": "^0.4.4",
    "@types/eslint__js": "^8.42.3",
    "babel-jest": "^29.7.0",
    "eslint": "^9.7.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "globals": "^15.8.0",
    "jest": "^29.7.0",
    "prettier": "^3.3.3",
    "rollup": "^4.19.0",
    "rollup-plugin-license": "^3.5.2",
    "rollup-plugin-scss": "^4.0.0",
    "rollup-plugin-typescript2": "^0.36.0",
    "sass": "^1.77.8",
    "tslib": "^2.6.3",
    "typescript": "^5.5.3",
    "typescript-eslint": "^7.16.1"
  }
}
